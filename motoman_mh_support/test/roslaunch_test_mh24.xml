<?xml version="1.0" ?>
<launch>
  <arg name="ip_str" value="127.0.0.1" />
  <arg name="dx100" value="dx100" />
  <arg name="fs100" value="fs100" />

  <group ns="load_mh24__">
    <include file="$(find motoman_mh_support)/launch/load_mh24.launch"/>
  </group>

  <group ns="test_mh24__">
    <include file="$(find motoman_mh_support)/launch/test_mh24.launch"/>
  </group>

  <group ns="robot_interface_download_mh24__">
    <group ns="$(arg dx100)">
      <include file="$(find motoman_mh_support)/launch/robot_interface_streaming_mh24.launch">
        <arg name="robot_ip" value="$(arg ip_str)" />
        <arg name="controller" value="$(arg dx100)" />
      </include>
    </group>
    <group ns="$(arg fs100)">
      <include file="$(find motoman_mh_support)/launch/robot_interface_streaming_mh24.launch">
        <arg name="robot_ip" value="$(arg ip_str)" />
        <arg name="controller" value="$(arg fs100)" />
      </include>
    </group>
  </group>

  <group ns="robot_state_visualize_mh24__">
    <group ns="$(arg dx100)">
      <include file="$(find motoman_mh_support)/launch/robot_state_visualize_mh24.launch">
        <arg name="robot_ip" value="$(arg ip_str)" />
        <arg name="controller" value="$(arg dx100)" />
      </include>
    </group>
    <group ns="$(arg fs100)">
      <include file="$(find motoman_mh_support)/launch/robot_state_visualize_mh24.launch">
        <arg name="robot_ip" value="$(arg ip_str)" />
        <arg name="controller" value="$(arg fs100)" />
      </include>
    </group>
  </group>
</launch>
