<launch>
  <arg name="ip_str" value="127.0.0.1" />

  <group ns="load_es_165_rd_ii__">
    <include file="$(find motoman_es_165_rd_ii_support)/launch/load_es_165_rd_ii.launch"/>
  </group>

  <group ns="test_es_165_rd_ii__">
    <include file="$(find motoman_es_165_rd_ii_support)/launch/test_irb2400.launch"/>
  </group>

  <group ns="robot_interface_download_es_165_rd_ii__">
    <include file="$(find motoman_es_165_rd_ii_support)/launch/robot_interface_download_es_165_rd_ii.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>

  <group ns="robot_state_visualize_es_165_rd_ii__">
    <include file="$(find motoman_es_165_rd_ii_support)/launch/robot_state_visualize_es_165_rd_ii.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>
